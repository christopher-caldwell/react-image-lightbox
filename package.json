{
  "name": "@caldwell619/react-image-lightbox",
  "version": "0.0.1",
  "description": "A lightbox component for React",
  "scripts": {
    "lint": "eslint --ext .ts,.tsx src",
    "format": "prettier --write src",
    "build": "tsc && npx tscpaths -p tsconfig.json -s src -o dist && cp src/style.css dist",
    "build:watch": "nodemon --exec npm run build --watch src --ext .tsx,.ts --delay 2.5",
    "publish:release": "sh publish.sh",
    "predeploy": "cd demo && yarn install && yarn run build",
    "deploy": "gh-pages -d demo/dist"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "homepage": "https://github.com/christopher-caldwell/react-image-lightbox",
  "repository": {
    "type": "git",
    "url": "https://github.com/christopher-caldwell/react-image-lightbox.git"
  },
  "bugs": "https://github.com/christopher-caldwell/react-image-lightbox/issues",
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-modal": ">=3.15.1"
  },
  "devDependencies": {
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.1",
    "@mui/material": "^5.2.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.5.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "gh-pages": "^3.2.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.5.1",
    "rc": "^1.2.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-modal": "^3.15.1",
    "standard-version": "^9.3.2",
    "tscpaths": "^0.0.9",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@types/react-modal": "^3.13.1"
  },
  "keywords": [
    "react",
    "react-component",
    "image",
    "lightbox",
    "modal"
  ]
}
